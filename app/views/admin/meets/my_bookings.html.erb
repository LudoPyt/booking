<div class="corps">
<h1><span>Mes</span><span>rendez-vous</span></h1>
  <%= link_to "Prendre un autre rdv", new_meet_path, class: "btn btn-primary text-center", id: "prendre" %>
  <container>
      <h3> Voici la liste des rendez-vous que vous avez.</h3>
      <% @meets.each do |meet| %>
      <%= link_to admin_meet_path(meet.id) do %>
    	<div class="button">
          <h3 class="text-center">
            <% if current_user.first_name == meet.user.first_name && current_user.last_name == meet.user.last_name %>
              <%= meet.first_name %> <%= meet.last_name %>
            <% else %>
              <%= meet.user.first_name %> <%= meet.user.last_name %>
            <% end %>
          </h3>
          <p class="text-center"><%= meet.date %></p>
    	</div>
        <% end %>
      <% end %>


  </container>
</div>
